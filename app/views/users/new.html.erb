

	<%=bootstrap_form_for(@user) do |f| %>
  <div class="col-2">
    <label>
      <%=f.text_field :first_name %>
    </label>
  </div>
  <div class="col-2">
    <label>     
      <%=f.text_field :last_name %>
    </label>
  </div>
  <div class="col-3">
    <label>     
     <%=f.text_field :email %>
    </label>
  </div>
  <div class="col-3">
    <label>     
	    <%=f.password_field :password %>
    </label>
  </div>
  <div class="col-3">
  	<label>
     	<%=f.text_field :st_num %>
    </label>
  </div>
   <div class="col-2">
    <label>
      <%=f.text_field :st_name %>
    </label>
  </div>
   <div class="col-2">
    <label>
  		<%=f.text_field :city %>
    </label>
  </div>
   <div class="col-2">
    <label>
      <%=f.text_field :state %>
    </label>
  </div> <div class="col-2">
    <label>   
     <%=f.text_field :zip %>
    </label>
  </div>
  <div>
  	<%=f.hidden_field :longitude %>
	<%=f.hidden_field :latitude %>
  </div>
  <%if @errors %>
		<%@errors %>
	<% end %> 
  <div class="col-submit">	
    <%=f.submit "Register"%>
  </div>
  <% end %> 
